<launch> 
	<!-- Robot Driver -->
	<include file="$(find ur_robot_driver)/launch/ur5e_bringup.launch">
		<arg name="robot_ip" value="127.0.0.1"/>
	</include>

	<!-- Camera and AR -->
	<include file="$(find usb_cam)/launch/usb_cam-test.launch"/> 
	<include file="$(find ur5_e_moveit_config)/launch/move_group.launch"/>

	<!-- Rviz -->
	<include file="$(find ur5_e_moveit_config)/launch/moveit_rviz.launch">
		<arg name="config" value="true"/>
	</include>

	<!-- Services -->
	<include file="$(find lds)/launch/services.launch"/>


</launch>

